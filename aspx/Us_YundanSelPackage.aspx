<script> 
function showlianxi(status,ev,remarkstr){ 
var  e= ev||window.event; 
var show = document.getElementById('showlianxi'); 
var obj=e.target||e.srcElement; 
switch(status){ 
case 'over': 
show.style.position='absolute'; 
if(e.pageX){ 
document.getElementById('layer_text').innerHTML = remarkstr;
show.style.left=e.pageX+'px'; 
show.style.top=e.pageY+'px'; 
} 
else{ 
document.getElementById('layer_text').innerHTML = remarkstr;
show.style.left=e.clientX; 
show.style.top=e.clientY; 
} 
show.style.display='block'; 
break; 
case 'out': 
show.style.display='none'; 
document.getElementById('layer_text').innerHTML = "";
break; 
} 
} 

</script> 
