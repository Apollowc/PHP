<script>

    function selpackage(tempid,tempweight,temptext)
    {
        var weightstr = 0;
        
        var idstr = "";
        
        if(document.getElementById("yd_package_"+tempid).checked)
        {
        
            //原箱转运
            if(parent.document.getElementById("bs_type").value=="0")
            {
                idstr_usps = parent.document.getElementById("yw_packagesel").value;
                
                if(parent.document.getElementById("bs_type_full")==null)
                {
                    if(idstr_usps.split(",").length>1)
                    {
                        alert("原箱转运只可以选择一个包裹！");
                        return false;
                    }
                }
                
            }
            
            if(parent.document.getElementById("selpackageshow").innerHTML =="暂无已选包裹")
            {
                parent.document.getElementById("selpackageshow").innerHTML = "";
            }
            parent.document.getElementById("selpackageshow").innerHTML =  parent.document.getElementById("selpackageshow").innerHTML + "<BR><BR>"+temptext  ;
            
            weightstr = parent.document.getElementById("yw_packagefullweight").value;
            parent.document.getElementById("yw_packagefullweight").value = changeTwoDecimal(parseFloat(weightstr) + parseFloat(tempweight));
            
            parent.document.getElementById("yw_packagesel").value = parent.document.getElementById("yw_packagesel").value + tempid + ",";
        }
        else
        {
            idstr = parent.document.getElementById("yw_packagesel").value;
            
            if(idstr.split(",").length>2)
            {
                //合箱转运
                if(parent.document.getElementById("bs_type").value=="1")
                {
                    if(idstr.split(",").length=="3")
                    {
                        alert("合箱服务您最少需要选择二个包裹");
                        return false;
                    }
                }
                var tempstr = parent.document.getElementById("selpackageshow").innerHTML;
                tempstr = tempstr.replace(String("<BR><BR>"+temptext),"");
                parent.document.getElementById("selpackageshow").innerHTML = tempstr;
                    
                weightstr = parent.document.getElementById("yw_packagefullweight").value;
                parent.document.getElementById("yw_packagefullweight").value = changeTwoDecimal(parseFloat(weightstr) - parseFloat(tempweight));
                    
                //idstr = parent.document.getElementById("yw_packagesel").value;
                idstr = idstr.replace(tempid+",","");
                parent.document.getElementById("yw_packagesel").value = idstr;
            }
            else
            {
                alert("您最少需要选择一个包裹");
                return false;
            }
        }
        
        if(parent.document.getElementById("tw_package_willprice")!=null)
        {
            window.parent.willprice_fuc();
        }
        
        return true;                                          
    }


var editbsno = "";

